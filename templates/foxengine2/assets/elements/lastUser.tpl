<div id="profileContents" style="background: linear-gradient(45deg, #c5c5e19c, {colorScheme});">
    <table>
        <tr>
            <td>
                <img class="profilePhoto" src="{profilePhoto}" style="width: 64px;" alt="{login}">
            </td>
            <td>
                <div class="profile-title">
                    <ul>
                        <li><h1><a href="#" onclick="foxEngine.user.showUserProfile('{login}'); return false;">{login}</a></h1></li>
                        <li><span>{realname}</span></li>
                        <li><span class="groupStatus-4">{regDate}</span></li>
                    </ul>
                </div>
            </td>
        </tr>
    </table>
</div>
