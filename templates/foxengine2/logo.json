{
	"timeline": [
					{
						"targets": ".logoWrapper .logo ul",
						"opacity": [0, 1]
					}, 
					{
						"targets": ".logo img",
						"translateY": [-100, 0],
						"opacity": [0, 1],
						"elasticity": 600,
						"duration": 1000
					},
					{
						"targets": ".logo .letter",
						"opacity": [0, 1],
						"translateY": [60, 0],
						"duration": 800,
						"delay": 100
					},
					{
						"targets": ".logo .letterStatus",
						"opacity": [0, 1],
						"easing": "easeInOutExpo",
						"duration": 100,
						"delay": 0
					},
					{
						"targets": ".logo .status",
						"opacity": [0, 1],
						"translateY": [-60, 0],
						"scaleX": 1,
						"elasticity": 500,
						"duration": 1000,
						"delay":  200
					},
					{
						"targets": ".logo .line",
						"scaleX": [0.4, 1],
						"easing": "easeInOutExpo",
						"duration": 2000
					},
					{
						"targets": ".logo",
						"translateY": [0, -50],
						"opacity": [1, 0],
						"easing": "easeInOutExpo",
						"duration": 1000,
						"delay": 1000
					}
				], 
				 "construct": {
					 "loop": false,
					 "autoplay": true
				}
		}