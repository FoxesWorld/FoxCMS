 <div class="module">
	<span class="moduleSettings module-{moduleName}">
		<i class="fa fa-cogs" aria-hidden="true"></i>
	</span>
	<b class="moduleName">{moduleName}</b>
	<img class="modulePriority" alt="{modulePriority}" src="{priorImg}" />

	<ul class="moduleContents">
		<table>
			<td>
				<li><img class="modulePicture" src="{modulePicture}" /></li>
			</td>

			<td>
				<li><b class="moduleKey">ModuleGroups: </b> <b class="moduleValue">{moduleGroups}</b></li>
				<li><b class="moduleKey">Version: </b> <b class="moduleValue">{version}</b></li>
				<li><b class="moduleKey">Description: </b> <b class="moduleValue">{description}</b></li>
				<li><b class="moduleKey">Priority: </b> <b class="moduleValue">{modulePriority}</b></li>
				<li><b class="moduleKey">Mainclass: </b> <b class="moduleValue">{moduleMainClass}</b></li>
			</td>
		</table>
	</ul>
</div>
